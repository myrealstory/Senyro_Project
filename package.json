{
  "name": "senryo-web",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "//": "README -> npm script",
    "//example": "npm run docker-compose-up:dev --build_env=dev",
    "dev:start": "env-cmd -f .env.local npm run build && npm run start",
    "dev": "next dev",
    "dev:https": "next dev --experimental-https",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "dev:debug": "NODE_OPTIONS='--inspect' next dev",
    "docker-compose-up:dev": "VERSION=$(git rev-parse --short HEAD) NODE_ENV=development BUILD_ENV=${npm_config_build_env} docker-compose --env-file .env.${npm_config_build_env} -f docker-compose.yml -f docker-compose.dev.yml up --build",
    "docker-compose-up:prod": "VERSION=$(git rev-parse --short HEAD) NODE_ENV=production BUILD_ENV=${npm_config_build_env} docker-compose --env-file .env.${npm_config_build_env} up --build",
    "docker-compose-down": "docker-compose --env-file .env.${npm_config_build_env} down",
    "docker-build": "docker build -t senryo-${npm_config_build_env}:${npm_config_tag} . --build-arg BUILD_ENV=${npm_config_build_env} --build-arg NODE_ENV=${npm_config_node_env}",
    "docker-run": "docker run --name ${npm_config_container_name} -p 3000:3000 ${npm_config_image_name}",
    "docker-compose-up:debug": "NODE_OPTIONS='--inspect' VERSION=$(git rev-parse --short HEAD) NODE_ENV=${npm_config_node_env} BUILD_ENV=${npm_config_build_env} docker-compose --env-file .env.${npm_config_build_env} up --build",
    "prepare": "husky install"
  },
  "dependencies": {
    "@formatjs/intl-localematcher": "^0.2.32",
    "@reduxjs/toolkit": "^1.9.5",
    "@tailwindcss/forms": "^0.5.4",
    "@types/jsonwebtoken": "^9.0.3",
    "@types/negotiator": "^0.6.1",
    "@use-gesture/react": "^10.3.0",
    "accept-language": "^3.0.18",
    "all": "^0.0.0",
    "autoprefixer": "^10.4.14",
    "cookies-next": "^2.1.2",
    "embla-carousel-react": "^8.0.0-rc11",
    "env-cmd": "^10.1.0",
    "google-maps-react-markers": "^1.4.2",
    "html2canvas": "^1.4.1",
    "i18next": "^22.5.0",
    "i18next-browser-languagedetector": "^7.0.1",
    "i18next-resources-to-backend": "^1.1.4",
    "jsonwebtoken": "^9.0.2",
    "jspdf": "^2.5.1",
    "lodash": "^4.17.21",
    "lodash.debounce": "^4.0.8",
    "moment": "^2.29.4",
    "negotiator": "^0.6.3",
    "next": "^13.5.4",
    "next-qrcode": "^2.5.1",
    "postcss-nesting": "^12.0.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-google-recaptcha": "^3.1.0",
    "react-i18next": "^12.3.1",
    "react-icons": "^4.8.0",
    "react-qr-code": "^2.0.12",
    "react-qrcode-logo": "^2.9.0",
    "react-redux": "^8.1.0",
    "react-spring-bottom-sheet": "^3.5.0-alpha.0",
    "react-swipeable-views": "^0.14.0",
    "react-transition-group": "^4.4.5",
    "react-use-draggable-scroll": "^0.4.7",
    "redux": "^4.2.1",
    "redux-logger": "^3.0.6",
    "sass": "^1.69.3",
    "server-only": "^0.0.1",
    "sharp": "^0.32.4",
    "swiper": "^10.2.0",
    "tailwindcss": "^3.3.2",
    "typescript": "^5.2.2",
    "uuid": "^9.0.1",
    "cssnano": "^6.0.3"
  },
  "devDependencies": {
    "@types/google-map-react": "^2.1.7",
    "@types/lodash.debounce": "^4.0.8",
    "@types/node": "18.15.1",
    "@types/react": "18.0.28",
    "@types/react-dom": "18.0.11",
    "@types/react-google-recaptcha": "^2.1.6",
    "@types/react-transition-group": "^4.4.10",
    "@types/redux-logger": "^3.0.10",
    "@types/uuid": "^9.0.4",
    "@typescript-eslint/eslint-plugin": "^5.59.6",
    "@typescript-eslint/parser": "^5.59.6",
    "eslint": "^8.36.0",
    "eslint-config-next": "^13.4.2",
    "eslint-config-prettier": "^8.8.0",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.3",
